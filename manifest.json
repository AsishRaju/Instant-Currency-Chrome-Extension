{
    "manifest_version": 2,
    "name": "Instant Currency",
    "version": "1.0",
    "description": "Covert any currency to your local currency Instantly!",
    "icons" :{
        "128": "img/icon128.png",
        "48": "img/icon48.png",
        "16": "img/icon16.png"
    },
    "browser_action":{
        "default_icon": "img/icon16.png",
        "default_popup": "popup.html"
    },
    "permissions":[
        "activeTab",
        "http://*/",
        "https://*/"
    ],
    "commands": {
        "_execute_browser_action": {
          "suggested_key": {
            "default": "Alt+C",
            "linux": "Alt+C",
            "windows": "Alt+C",
            "mac": "Alt+C"
          }
        }
      }

}